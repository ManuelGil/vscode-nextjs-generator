{
  "name": "vscode-nextjs-generator",
  "displayName": "React / NextJS / T3 Stack File Generator",
  "description": "VSCode extension to generate files for T3 Stack project. NextJS, NextAuth, Prisma, tRPC, and more. Create pages, components, layouts, classes, interfaces, and more.",
  "version": "0.3.0",
  "preview": true,
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#52137b",
    "theme": "dark"
  },
  "license": "MIT",
  "publisher": "imgildev",
